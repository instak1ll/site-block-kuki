{
    "manifest_version": 2,
    "name": "Bloqueador de enlaces",
    "version": "1.0",
    "description": "Una extensi√≥n para bloquear enlaces.",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "background": {
      "persistent": true,
      "scripts": ["background.js"]
  },
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }